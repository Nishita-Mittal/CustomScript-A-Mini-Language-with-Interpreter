// Variable declarations and arithmetic
var a = 3;
var b = 7;
var sum = a + b;
print(sum); // Should print 10

// If-else statement
if (sum > 5) {
    print(1);
} else {
    print(0);
}

// While loop
var i = 1;
var fact = 1;
while (i <= 5) {
    fact = fact * i;
    i = i + 1;
}
print(fact); // Should print 120

// For loop
var total = 0;
for (var j = 1; j <= 5; j = j + 1;) {
    total = total + j;
}
print(total); // Should print 15

// Nested if-else and arithmetic
if (a < b) {
    if (b - a == 4) {
        print(100);
    } else {
        print(200);
    }
} else {
    print(300);
}